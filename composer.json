{
    "name": "zfhassaan/zindagi-zconnect",
    "description": "Laravel Package for Zindagi Z-Connect JS Bank Integration - Onboarding, Inquiry, Payment, and Lending Solutions",
    "keywords": [
        "zindagi",
        "zconnect",
        "jsbank",
        "onboarding",
        "inquiry",
        "payment",
        "lending",
        "php",
        "laravel",
        "api",
        "banking"
    ],
    "type": "library",
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "laravel/pint": "^1.13"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "zfhassaan\\ZindagiZconnect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Hassaan Ali",
            "email": "zfhassaan@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "zfhassaan\\ZindagiZconnect\\Providers\\ZindagiZconnectServiceProvider"
            ],
            "aliases": {
                "ZindagiZconnect": "zfhassaan\\ZindagiZconnect\\Facades\\ZindagiZconnect"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

